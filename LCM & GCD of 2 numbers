a=int(input("enter 1st number:"))
b=int(input("enter 2nd number:"))

if a>b:
    max=a
    min=b
else:
    max=b
    min=a
    
for i in range(1,min+1):
    if (min%i==0 and max%i==0):
        gcd=i
print(f"GCD of {a} and {b} is {gcd}")        

lcm = (a*b)/gcd
print(f"LCM of {a} and {b} is {lcm}")
