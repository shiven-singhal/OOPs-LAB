print("ENTER MARKS OF 5 SUBJECTS")
s=0
for i in range(1,6,1):
    n = int(input())
    s=s+n
    
p=s/5
print("PERCENTAGE = ",p)
if p>90.0:
    print("A+ GRADE")
elif p>80.0:
    print("A GRADE")
elif p>70.0:
    print("B GRADE")
elif p>60.0:
    print("B+ GRADE")    
elif p>50.0:
    print("C GRADE")    
elif p>40.0:
    print("D GRADE")    
elif p>35.0:
    print("E GRADE")    
else:
    print("FAIL")
    
